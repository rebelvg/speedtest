{
  "name": "speedtest",
  "version": "1.0.0",
  "repository": "https://git.klpq.men/klpq/speedtest.git",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "api",
    "app"
  ],
  "scripts": {
    "prepush": "prettier --write \"**/*.js\"",
    "build:app": "webpack --config webpack.config.production --mode production",
    "start:api": "yarn workspace api start",
    "start:app": "webpack-dev-server --mode development",
    "start": "concurrently --kill-others \"yarn run start:api\" \"yarn run start:app\""
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "prettier": "^1.15.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {}
}
