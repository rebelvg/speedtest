{
  "name": "speedtest",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "api",
    "app"
  ],
  "scripts": {
    "prepush": "yarn run lint",
    "lint": "prettier --list-different \"**/*.{ts,js,json,css,md}\"",
    "lint:fix": "prettier --write \"**/*.{ts,js,json,css,md}\"",
    "build:app": "webpack --config webpack.config.production --mode production",
    "start:app": "webpack-dev-server --mode development",
    "start:api": "yarn workspace api start",
    "start": "concurrently --kill-others \"yarn run start:api\" \"yarn run start:app\""
  },
  "devDependencies": {
    "@types/node": "^14.14.0",
    "concurrently": "^3.5.1",
    "prettier": "^1.15.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "dependencies": {}
}
